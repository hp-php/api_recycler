<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">

    <size
        android:height="100dp"
        android:width="100dp"/>

    <solid android:color="@color/White"/>

    <stroke android:width="1dp"
        android:color="@color/White"
        />

    <corners
        android:radius="100dp" />
